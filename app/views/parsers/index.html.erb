<h1>All parsed logs</h1>

<ol>
    <% @events.each do |event| %>
    <li><%= event.data %></li>
    
    <ul>
    <li>Source IP : <%= event.data.match(/src=((\d{1,3}\.){3}\d{1,3})/)[1] %></li>
    <li>Destination IP : <%= event.data.match(/dst=((\d{1,3}\.){3}\d{1,3})/)[1] %></li>
    </ul><br/>
    <%end  %>

</ol>





